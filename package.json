{
  "name": "gemini-history-manager",
  "version": "0.19.1",
  "description": "Track and manage your Google Gemini chat history with advanced visualization and organization tools.",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build -b chrome",
    "build:firefox": "wxt build --mv3 -b firefox",
    "zip": "wxt zip -b chrome",
    "zip:firefox": "wxt zip --mv3 -b firefox",
    "release": "node scripts/release.js",
    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare",
    "lint": "wxt lint",
    "format": "pretty-quick --staged",
    "format:check": "pretty-quick --check --staged",
    "format:all": "prettier --write \"**/*.{js,vue,json,css,md,html}\"",
    "format:check:all": "prettier --check \"**/*.{js,vue,json,css,md,html}\"",
    "prepare": "husky"
  },
  "keywords": [
    "gemini",
    "history",
    "browser-extension",
    "firefox-addon",
    "chrome-extension",
    "vue"
  ],
  "author": "InvictusNavarchus",
  "license": "GPL-3.0",
  "private": true,
  "devDependencies": {
    "@wxt-dev/auto-icons": "^1.1.1",
    "@wxt-dev/module-vue": "^1.0.3",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.2.5",
    "wxt": "^0.20.18"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "dayjs": "^1.11.10",
    "minisearch": "^7.1.2",
    "vue": "^3.4.27"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
